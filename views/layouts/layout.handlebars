<!DOCTYPE html>
<html>
<head>
	<title>Smart Dashboard</title>
	<link rel="stylesheet" href="/css/bootstrap.css" />
	<link rel="stylesheet" href="/css/style.css" />
	{{#if user}}
		{{#if v_qses}}
			<link rel="stylesheet" href="http://127.0.0.1/resources/autogenerated/qlik-styles.css">
		{{/if}}
	<script src="http://127.0.0.1/resources/assets/external/requirejs/require.js"></script>
	<script src="/js/qlik_mashup.js"></script>
	{{/if}}
	
</head>
<body role="document" style="overflow: auto">
	<div class="container"> 
        <nav>
          <ul class="nav nav-pills pull-right">
            {{#if user}}
			<!--
              <li role="presentation"><a href="/">Dashboard</a></li>
			  <li role="presentation"><a href="/details">Details</a></li>
			  -->
              <li role="presentation"><a href="/users/logout" id="logout">Logout</a></li>
            {{else}}
              <li role="presentation"><a href="/users/login">Login</a></li>
              <li role="presentation"><a href="/users/register">Register</a></li>
            {{/if}}
          </ul>
        </nav>
      <h3><b>Energy consumption analysis</b></h3>
      <div class="row">
        <div class="col-lg-12">
          {{#if success_msg}}
            <div class="alert alert-success">{{success_msg}}</div>
          {{/if}}

          {{#if error_msg}}
            <div class="alert alert-danger">{{error_msg}}</div>
          {{/if}}

          {{#if error}}
            <div class="alert alert-danger">{{error}}</div>
          {{/if}}
		  
        </div>
      </div>
	 
		{{{body}}}
	
      <footer class="footer">
        <p>&copy; 2017 Smart Dashboard, Inc.</p>
      </footer>

    </div> <!-- /container -->
</body>
</html>
